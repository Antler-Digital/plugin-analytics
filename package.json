{
  "name": "payload-plugin-analytics",
  "version": "1.0.53",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./rsc": {
      "import": "./dist/exports/rsc.js",
      "types": "./dist/exports/rsc.d.ts",
      "default": "./dist/exports/rsc.js"
    },
    "./styles/index.css": {
      "import": "./dist/styles/index.css",
      "types": "./dist/styles/index.css",
      "default": "./dist/styles/index.css"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:styles && pnpm copyfiles && pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc --outDir dist --rootDir ./src",
    "build:styles": "tailwindcss -i ./src/styles/input.css -o ./src/styles/index.css",
    "clean": "rimraf {dist,*.tsbuildinfo}",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "dev": "concurrently \"pnpm dev:styles\" \"payload run ./dev/server.ts\"",
    "dev:styles": "tailwindcss -i ./src/styles/input.css -o ./src/styles/index.css --watch",
    "dev:generate-importmap": "pnpm dev:payload generate:importmap",
    "dev:generate-types": "pnpm dev:payload generate:types",
    "dev:payload": "cross-env PAYLOAD_CONFIG_PATH=./dev/payload.config.ts payload",
    "lint": "eslint",
    "lint:fix": "eslint ./src --fix",
    "prebuild": "pnpm clean",
    "prepublishOnly": "pnpm clean && pnpm build",
    "test": "echo 'test'",
    "release": "pnpm build && changeset version && changeset publish",
    "prepare": "husky",
    "cz": "git-cz"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/cli": "^4.0.14",
    "@tailwindcss/postcss": "^4.0.14",
    "@tanstack/react-table": "^8.21.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "esbuild": "^0.25.0",
    "lucide-react": "^0.475.0",
    "mongodb": "^6.13.0",
    "next": "15.1.5",
    "postcss": "^8.5.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-simple-maps": "^3.0.0",
    "recharts": "^2.15.1",
    "swr": "^2.3.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.0",
    "@eslint/eslintrc": "^3.2.0",
    "@payloadcms/db-mongodb": "3.17.1",
    "@payloadcms/db-postgres": "3.17.1",
    "@payloadcms/db-sqlite": "3.17.1",
    "@payloadcms/eslint-config": "3.9.0",
    "@payloadcms/next": "3.17.1",
    "@payloadcms/richtext-lexical": "3.17.1",
    "@payloadcms/ui": "3.17.1",
    "@swc-node/register": "1.10.9",
    "@swc/cli": "0.6.0",
    "@swc/jest": "^0.2.37",
    "@types/jest": "29.5.12",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@types/react-simple-maps": "^3.0.6",
    "concurrently": "^9.1.2",
    "copyfiles": "2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.5",
    "git-cz": "^4.9.0",
    "graphql": "^16.8.1",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "mongodb-memory-server": "^10.1.2",
    "open": "^10.1.0",
    "payload": "3.17.1",
    "prettier": "^3.4.2",
    "qs-esm": "7.0.2",
    "rimraf": "3.0.2",
    "sharp": "0.32.6",
    "sort-package-json": "^2.10.0",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "payload": "^3.17.1"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./rsc": {
        "import": "./dist/exports/rsc.js",
        "types": "./dist/exports/rsc.d.ts",
        "default": "./dist/exports/rsc.js"
      },
      "./styles": {
        "import": "./dist/styles/output.css",
        "types": "./dist/styles/output.css",
        "default": "./dist/styles/output.css"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "registry": "https://registry.npmjs.org/"
}
